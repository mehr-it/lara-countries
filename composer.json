{
  "name": "mehr-it/lara-countries",
  "description": "Extendable multilingual, countries list for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Mieger",
      "email": "mail@mehr-it.info"
    }
  ],
  "autoload": {
    "psr-4": {
      "MehrIt\\LaraCountries\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MehrItLaraCountriesTest\\": "test/"
    }
  },
  "require": {
    "php": ">=7.1",
    "laravel/framework": "^5.8|^6.0|^7.0",
    "umpirsky/country-list": "^2.0",
    "umpirsky/language-list": "^1.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4|^8.0",
    "orchestra/testbench": "^3.8|^4.0|^5.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MehrIt\\LaraCountries\\Provider\\CountriesServiceProvider"
      ],
      "aliases": {
        "Countries": "MehrIt\\LaraCountries\\Facades\\Countries",
        "Languages": "MehrIt\\LaraCountries\\Facades\\Languages"
      }
    }
  }
}
